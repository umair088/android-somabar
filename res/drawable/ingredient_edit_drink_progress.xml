<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape>
            

            <gradient
                android:angle="270"
                android:centerColor="#ABADC1"
                android:centerY="0.5"
                android:endColor="#ABADC1"
                android:startColor="#ABADC1" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                

                <gradient
                    android:angle="0"
                    android:endColor="#31333F"
                    android:startColor="#31333F" />
            </shape>
        </clip>
    </item>

</layer-list>